    "def shortcut( s ):\n",
    "    list = []\n",
    "    str = ''\n",
    "    vowel = [\"a\",\"e\",\"i\",\"o\",\"u\"]\n",
    "    for i in s:\n",
    "        if i not in vowel:\n",
    "            list.append(i)\n",
    "    str = str.join(list)\n",
    "    return str"
   ]
